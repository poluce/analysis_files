root = true

[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true
indent_style = space
indent_size = 4

[*.bat]
end_of_line = crlf

[*.md]
trim_trailing_whitespace = false

[*.pro]
indent_size = 4

[*.{h,hpp,hxx,c,cc,cpp,cxx}]
indent_size = 4
indent_style = space
